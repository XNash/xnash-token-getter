{
  "manifest_version": 3,
  "name": "XNash Token Getter",
  "version": "1.0",
  "description": "Affiche les cookies du site actuel",
  "permissions": ["cookies", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
